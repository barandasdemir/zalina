<% if (orders && orders.length > 0) { %>
<table class="col s6 offset-s1">
    <thead>
        <tr>
            <th>Sipariş Numarası</th>
            <th>Sipariş Tarihi</th>
            <th>Ürün Adedi</th>
            <th></th>
        </tr>
    </thead>

    <tbody>
        <% orders.forEach(order => { %>
        <tr>
            <td><%= order.id %></td>
            <td><%= order.date %></td>
            <td><%= order.qty %></td>
            <td>
                <a href="/profile/taken/<%= order.id %>">
                    <i class="material-icons black-text">send</i>
                </a>
            </td>
        </tr>
        <% }); %>
    </tbody>
</table>
<% } else { %>
    <h5 class="col s6 offset-s1 center">Hiç siparişiniz yok!</h5>
<% } %>
